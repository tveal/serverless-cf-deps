{
  "name": "cf-deps",
  "version": "1.3.0",
  "description": "cli tool to generate and publish CloudFormation dependencies from yml files",
  "bin": {
    "cf-deps": "lib/cf-deps.js",
    "cf-groups": "lib/cf-groups.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint --fix test/**/*.js lib/*.js",
    "test": "npm run lint && jest"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./lib/**"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 100,
        "functions": 100,
        "branches": 100,
        "statements": 100
      }
    }
  },
  "author": "TVeal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tveal/serverless-cf-deps.git"
  },
  "homepage": "https://github.com/tveal/serverless-cf-deps#readme",
  "bugs": {
    "url": "https://github.com/tveal/serverless-cf-deps/issues"
  },
  "keywords": [
    "cf-deps",
    "cf-groups",
    "aws",
    "cloudformation",
    "dependencies",
    "yaml",
    "yml",
    "serverless"
  ],
  "dependencies": {
    "ansi-regex": "^5.0.1",
    "chalk": "^4.0.0",
    "clui": "^0.3.6",
    "debug": "^4.3.6",
    "es5-ext": "^0.10.64",
    "find-in-files": "^0.5.0",
    "ini": "^4.1.3",
    "lodash": "^4.17.21",
    "remote-origin-url": "^2.0.0",
    "rimraf": "^3.0.2",
    "simple-git": "^3.25.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0"
  }
}
